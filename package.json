{
    "name": "@j-cake/jcake-utils",
    "description": "A collection of utilities for everyday nodejs usage.",
    "author": {
        "name": "J-Cake",
        "email": "jakieschneider13@gmail.com",
        "url": "https://jschneiderprojects.com.au"
    },
    "repository": "https://github.com/J-Cake/jcake-utils",
    "type": "module",
    "version": "v2.0.1",
    "main": "./build/index.js",
    "typings": "lib",
    "scripts": {
        "build": "$npm_execpath exec mkjson rebuild",
        "postinstall": "$npm_execpath --prefix test install && $npm_execpath exec mkjson all"
    },
    "dependencies": {
        "chalk": "latest",
        "lodash": "latest"
    },
    "devDependencies": {
        "@types/node": "latest",
        "@types/lodash": "latest",
        "@j-cake/mkjson": "latest",
        "typescript": "latest",
        "esbuild": "latest"
    },
    "bin": {
        "dbrepl": "./build/dbrepl.js"
    },
    "typesVersions": {
        "*": {
            "exported": [
                "lib",
                "def",
                "build/lib",
                "build/def"
            ]
        }
    },
    "imports": {
        "#args": "./build/args.js",
        "#buffer": "./build/buffer.js",
        "#db": "./build/db.js",
        "#io": "./build/io.js",
        "#iter": "./build/iter.js",
        "#state": "./build/state.js",
        "#msg": "./build/msg.js",
        "#parse": "./build/parse.js"
    },
    "exports": {
        "./": "./build/index.js",
        "./args": "./build/args.js",
        "./buffer": "./build/buffer.js",
        "./db": "./build/db.js",
        "./io": "./build/io.js",
        "./iter": "./build/iter.js",
        "./state": "./build/state.js",
        "./msg": "./build/msg.js",
        "./parse": "./build/parse.js"
    }
}
